import java.util.Scanner;
public class Q3 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a[]=new int[6];
		
		for(int i=0;i<a.length;i++) {
			System.out.println("enter values");
			a[i]=sc.nextInt();
		}
		
		for(int i=0;i<a.length;i++) {
			if(a[i]%2==1) {
				System.out.println(a[i]);
			}
		}

	}

}
